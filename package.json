{
  "name": "truemlgpro_portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run build:watch\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:css": "postcss styles/tailwind.css -o styles/main.css",
    "build:watch": "postcss styles/tailwind.css -o styles/main.css --watch",
    "prod:build": "NODE_ENV=production npm run build:css && npm run build"
  },
  "dependencies": {
    "@react-three/drei": "^9.56.18",
    "@react-three/fiber": "^8.11.0",
    "autoprefixer": "^10.4.13",
    "default-passive-events": "^2.0.0",
    "next": "^13.1.6",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "three": "^0.149.0"
  },
  "devDependencies": {
    "cssnano": "^5.1.14",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6"
  }
}
